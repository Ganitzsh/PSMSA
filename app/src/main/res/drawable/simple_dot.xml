<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <size
                android:height="64dp"
                android:width="64dp" />
            <stroke
                android:color="@color/mainBlue"
                android:width="4dp"/>
        </shape>
    </item>
    <item
        android:gravity="center">
        <shape
            android:shape="oval">
            <padding android:bottom="12dp"/>
            <solid android:color="#ffffff"/>
            <size
                android:height="32dp"
                android:width="32dp" />
        </shape>
    </item>
</layer-list>